<core:FragmentDefinition xmlns="sap.m" xmlns:html="http://www.w3.org/1999/xhtml" xmlns:core="sap.ui.core">
	<List items="{path:'idConfigModel>/metadata/functionImport'}" growing="true" class="sapContrast sapContrastPlus" growingThreshold="7">
		<headerToolbar>
			<Toolbar>
				<SearchField placeholder="Seacrh Function Imports" liveChange='onPanelSearch'/>
			</Toolbar>
		</headerToolbar>
		<NotificationListItem class="sapUiSizeCompact" press="navToFunctionImport"
			title="{idConfigModel>name}"
			showCloseButton="false"
			description="{
				parts : [{path : 'idConfigModel>parameter'}, {path : 'idConfigModel>entityType'}],
				formatter : '.oFormatter.formatFunctionImportDesc'
			}"
			priority="Medium"
		>
		</NotificationListItem>
	</List>
</core:FragmentDefinition>