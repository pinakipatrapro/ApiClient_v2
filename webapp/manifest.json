{
	"_version": "1.8.0",
	"sap.app": {
		"id": "pinaki.sap.com.ApiClient",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"sourceTemplate": {
			"id": "ui5template.basicSAPUI5ApplicationProject",
			"version": "1.40.12"
		}
	},
	"sap.ui": {
		"technology": "UI5",
		"icons": {
			"icon": "",
			"favIcon": "",
			"phone": "",
			"phone@2": "",
			"tablet": "",
			"tablet@2": ""
		},
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		},
		"supportedThemes": [
			"sap_hcb",
			"sap_belize"
		]
	},
	"sap.ui5": {
		"rootView": {
			"viewName": "pinaki.sap.com.ApiClient.view.SplitApp",
			"type": "XML"
		},
		"dependencies": {
			"minUI5Version": "1.30.0",
			"libs": {
				"sap.m": {},
				"sap.ui.comp": {
					"minVersion": ""
				},
				"sap.ui.core": {},
				"sap.ui.layout": {}
			}
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "pinaki.sap.com.ApiClient.i18n.i18n"
				}
			}
		},
		"resources": {
			"css": [
				{
					"uri": "css/style.css"
				}
			]
		},
		"routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"viewType": "XML",
				"async": true,
				"viewPath": "pinaki.sap.com.ApiClient.view",
				"controlId": "idSplitApp",
				"bypassed": {
					"target": []
				}
			},
			"routes": [
				{
					"name": "Home",
					"pattern": "",
					"target": [
						"Slider",
						"Home"
					]
				},
				{
					"name": "Association",
					"pattern": "AssociationSets/{entitySet}",
					"target": [
						"Slider",
						"Association"
					]
				},
				{
					"name": "EntitySetData",
					"pattern": "EntitySetData/{entitySet}/{path}",
					"target": [
						"Slider",
						"EntitySetData"
					]
				},
				{
					"name": "CreateEntitySetRecord",
					"pattern": "CreateEntitySetRecord/{entitySet}/{mode}/{path}",
					"target": [
						"Slider",
						"CreateEntitySetRecord"
					]
				}
			],
			"targets": {
				"Slider": {
					"viewType": "XML",
					"transition": "slide",
					"clearAggregation": true,
					"viewName": "Slider",
					"viewLevel": 1,
					"controlAggregation": "masterPages"
				},
				"Home": {
					"viewType": "XML",
					"transition": "slide",
					"clearAggregation": true,
					"viewName": "Home",
					"viewLevel": 1,
					"controlAggregation": "detailPages"
				},
				"Association": {
					"viewType": "XML",
					"transition": "slide",
					"clearAggregation": true,
					"viewName": "Association",
					"viewLevel": 1,
					"controlAggregation": "detailPages"
				},
				"EntitySetData": {
					"viewType": "XML",
					"transition": "slide",
					"clearAggregation": true,
					"viewName": "EntitySetData",
					"viewLevel": 1,
					"controlAggregation": "detailPages"
				},
				"CreateEntitySetRecord": {
					"viewType": "XML",
					"transition": "slide",
					"clearAggregation": true,
					"viewName": "CreateEntitySetRecord",
					"viewLevel": 3,
					"controlAggregation": "detailPages"
				}
			}
		}
	},
	"sap.platform.hcp": {
		"uri": "webapp",
		"_version": "1.1.0"
	}
}